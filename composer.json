{
    "name": "orthosie/transliteration",
    "description": "Transliterate using unicode lookup.",
    "keywords": [
        "orthosie",
        "unicode",
        "transliterate"
    ],
    "homepage": "https://github.com/orthosie/transliteration",
    "license": "MIT",
    "authors": [
        {
            "name": "Realized Concepts",
            "homepage": "https://realizedconcepts.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Orthosie\\Unicode\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
